pipeline {
    agent any
    tools {
        maven 'Maven-3.9.5'
    }
    stages {
        // The Checkout stage has been removed.
        
        stage('Build & Test') {
            steps {
                sh 'mvn clean package'
            }
        }
        stage('Archive Artifact') {
            steps {
                archiveArtifacts artifacts: 'target/*.jar', fingerprint: true
            }
        }
    }
}
